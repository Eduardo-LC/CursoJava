public class TesteCarro {
	
	public static void main(String[] args) {
		
		Carro c = new Carro();
		
		System.out.println(c.velocidadeAtual);
		
		c.acelerar();
		c.acelerar();
		
		System.out.println(c.velocidadeAtual);
		
		c.frear();
		c.frear();
		c.frear();
		System.out.println(c.velocidadeAtual);
			
	}

}
