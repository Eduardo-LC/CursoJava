package streams;

public class Aluno {
	
	final String nome;
	final double nota1;
	final double nota2;
	final String sobreNome;
	
	
	public Aluno(String nome, double nota1, double nota2, String sobreNome) {
		super();
		this.nome = nome;
		this.nota1 = nota1;
		this.nota2 = nota2;
		this.sobreNome = sobreNome;
	}

}
