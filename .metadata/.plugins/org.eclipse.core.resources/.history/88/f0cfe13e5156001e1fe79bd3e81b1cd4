package oo.composicao.desafio;

import java.util.ArrayList;

public class Cliente {
	
	String nome;
	List<Compra> compras = new List<Compra>();
	
	public double obterValorTotal() {
		double total = 0;
		
		for(Compra compra: compras) {
			total += compra.obterValorTotal();
		}
		
		return total;
	}
		
		
}
