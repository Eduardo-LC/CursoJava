package classe;

public class DataTest {
	
	public static void main(String[] args) {
		
		Data d1 = new Data();
		Data d2 = new Data();
		
		d1.ano = 2012;
		d1.dia = 15;
		d1.mes = 02;
		
		d2.ano = 2015;
		d2.dia = 25;
		d2.mes = 12;

		System.out.printf(" %.0f/%.0f/%.0f " ,d1.dia , d1.mes ,d1.ano);
		System.out.printf("/n %.0f/%.0f/%.0f " ,d2.dia , d2.mes ,d2.ano);
	}

}
